{%- assign manualAcronyms = manual.acronyms -%}

<dl class="dl--acronyms">
  {%- for a in manualAcronyms -%}
    <dt id="{{ a.term | slugify }}">
      <abbr title="{{ a.definition | strip_html | strip_newlines | escape }}" class="initialism">
        {{ a.term }}
      </abbr>
    </dt>
    <dd>{{ a.definition }}</dd>
  {%- endfor -%}
</dl>